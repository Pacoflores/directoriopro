BEGIN:VCALENDAR
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME:betabeers
X-WR-TIMEZONE:Europe/Madrid
X-WR-CALDESC:{{title}}
{% for entity in entities %}
BEGIN:VEVENT
DTSTART:{{ entity.getGDateStart() }}
DTEND:{{ entity.getGDateEnd() }}
DTSTAMP:{{ entity.getGDateStart() }}
UID:{{ entity.id }}@betabeers.com
CREATED:{{ entity.getGDateStart() }}
DESCRIPTION:{{ url('event_show', { 'id': entity.id, 'slug': entity.slug }) }}
LAST-MODIFIED:{{ entity.getGDateStart() }}
LOCATION:{{entity.address}}, {{entity.location}}
SEQUENCE:0
STATUS:CONFIRMED
SUMMARY:{{ entity.title }}
TRANSP:OPAQUE
END:VEVENT
{% endfor %}
END:VCALENDAR