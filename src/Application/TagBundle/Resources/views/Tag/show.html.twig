{% extends "ApplicationAnunciosBundle::layout.html.twig" %}

{% block title %}Tag {{ title entity.title }}{% endblock %}


{% block sidebar %}

	
{% endblock %}



{% block content %}


	<h1>Tag {{ title entity.title }}</h1>

	<b>{{ entity.users }} Usuarios</b><br/><br/>
	
	{% for user in users %} 
		<a href="{{ path('user_show', { 'id': user.id, 'slug': user.slug }) }}" style="float:left;margin:0 10px 10px 0;width:240px">{{ user.name }}</a>
	{% endfor %}



{% endblock %}